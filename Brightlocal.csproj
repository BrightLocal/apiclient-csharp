<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RootNamespace>brightlocal</RootNamespace>
    <Company>BrigthLocal</Company>
    <Product>Brightlocal Api Client Csharp</Product>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageId>brightlocal.apiclient</PackageId>
    <Authors>BrigthLocal</Authors>
    <Description>BrightLocal API Client</Description>
    <Copyright>2021</Copyright>
    <PackageIcon></PackageIcon>
    <PackageProjectUrl>https://www.brightlocal.com/</PackageProjectUrl>
    <PackageTags>brightlocal api wrapper helper seo</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <Folder Include="Examples\" />
  </ItemGroup>

</Project>
